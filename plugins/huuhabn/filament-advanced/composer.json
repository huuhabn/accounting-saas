{
    "name": "huuhabn/filament-advanced",
    "description": "This is my package filament-advanced",
    "keywords": [
        "huuhabn",
        "laravel",
        "filament-advanced"
    ],
    "homepage": "https://github.com/huuhabn/filament-advanced",
    "support": {
        "issues": "https://github.com/huuhabn/filament-advanced/issues",
        "source": "https://github.com/huuhabn/filament-advanced"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "CyberSilk",
            "email": "cybersilk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "lab404/laravel-impersonate": "^1.7"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"

    },
    "autoload": {
        "psr-4": {
            "HanaSales\\FilamentAdvanced\\": "src/",
            "HanaSales\\FilamentAdvanced\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HanaSales\\FilamentAdvanced\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HanaSales\\FilamentAdvanced\\FilamentAdvancedServiceProvider"
            ],
            "aliases": {
                "FilamentAdvanced": "HanaSales\\FilamentAdvanced\\Facades\\FilamentAdvanced"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
