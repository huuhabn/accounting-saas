{
    "name": "hs/inventory",
    "description": "Inventory product management",
    "keywords": [
        "HS",
        "laravel",
        "inventory"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Huuhadev",
            "email": "huuhabn@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0",
        "lunarphp/admin": "self.version",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "HS\\Inventory\\": "src/",
            "HS\\Inventory\\Database\\Seeders\\": "database/seeders/",
            "HS\\Inventory\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HS\\Inventory\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HS\\Inventory\\InventoryServiceProvider"
            ],
            "aliases": {
                "Inventory": "HS\\Inventory\\Facades\\Inventory"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
